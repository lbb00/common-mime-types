    "web mime types",
    "MDN Common MIME Types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbb00/common-mime-types.git"
  },
  "author": "lbb00",
  "license": "Unlicense",
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.8"
  },
  "packageManager": "pnpm@10.22.0"
}
