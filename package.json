{
  "name": "common-mime-types",
  "version": "1.2.0",
  "description": "A lightweight TypeScript/JavaScript library for bidirectional conversion between file extensions and common web MIME types.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
